#include<bits/stdc++.h>
using namespace std;

int binarySearch(const vector<int>&vec,int key) //const will not change the vector ,& â†’ passes a reference (no copy)
{
    int low=0,high=vec.size()-1;
    while(low<=high){

        int mid=low+ (high-low)/2; //for avoiding overflow

        if(key<vec[mid]){
            high=mid-1;
        }
        else if(key>vec[mid]){
            low=mid+1;
        }
        else if(key==vec[mid]){
            return mid;
        }
    }
    return -1;

}

int main(){

vector<int>vec={1,2,3,4,5};
int target =2;

cout<< binarySearch(vec,target);

}
