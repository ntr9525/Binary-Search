#include<bits/stdc++.h>
using namespace std;

int binarySearch(int low,int high,int key,vector<int>vec){


while(low<=high){
int mid=(low+high)/2;
    if(key<vec[mid]){
        high=mid-1;
    }
    else if(key>vec[mid]){
        low=mid+1;
    }
    else if(vec[mid]==key){
        return mid;
    }


}
        return -1;
}

int main(){

vector<int>vec={1,2,3,4,5,6};
int target=5;
int low=0,high= vec.size()-1;
cout<<"the "<<target<<" is found at index " << binarySearch(low,high,target,vec);

}
