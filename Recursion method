#include<bits/stdc++.h>
using namespace std;

int binarySearch(vector<int>vec,int key,int high,int low)
{

if(low<=high){
    int mid=low + (high-low)/2;
    if(key>vec[mid]){
    return binarySearch(vec,key,high,mid+1);
    }
   else if(key<vec[mid]){
    return binarySearch(vec,key,mid-1,low);
    }
    else if(key==vec[mid]){
        return mid;
    }
}
    return -1;

}

int main(){

vector<int>vec={1,2,3,4,5};
int target =0;
int high=vec.size()-1,low=0;
cout<< binarySearch(vec,target,high,low);
return 0;
}
